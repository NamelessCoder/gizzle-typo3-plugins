{
    "name": "namelesscoder/gizzle-typo3-plugins",
    "description": "Gizzle plugins for running various tasks on repositories which contain TYPO3 extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Claus Due",
            "email": "claus@namelesscoder.net"
        }
    ],
	"autoload": {
		"psr-4": {
			"NamelessCoder\\GizzleTYPO3Plugins\\Tests\\Fixtures\\": "tests/fixtures/NamelessCoder/GizzleTYPO3Plugins/",
			"NamelessCoder\\GizzleTYPO3Plugins\\Tests\\Unit\\": "tests/unit/NamelessCoder/GizzleTYPO3Plugins/",
			"NamelessCoder\\GizzleTYPO3Plugins\\": "src/NamelessCoder/GizzleTYPO3Plugins/"
		}
	},
    "minimum-stability": "dev",
	"require": {
		"namelesscoder/gizzle": "1.0.*",
		"namelesscoder/gizzle-git-plugins": "dev-master",
		"namelesscoder/typo3-repository-client": "dev-master"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master",
		"mikey179/vfsStream": "~1.3.0",
		"phpunit/phpunit": "4.3.*"
	}
}
